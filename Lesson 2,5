# Задание 1

a = float(input('по шкале от 0 до 10, напишите, насколько Вы устали: '))
if a < 1:
    print('Кажется, Вы совсем не устали')
elif 1 <= a <= 5:
    print('Вы средненько устали')
elif 5 <= a <= 7.5:
    print('Вы очень устали')
elif 7.6 <= a <= 9:
    print ('Скорее идите отдыхать!')
elif 9 <= a <= 10:
    print ('Срочно смотреть котиков!')
else:
    print('Очень смешно! Прочитайте условие и введите циферку заново!')
    
# Задание 2
    
x = 0
y = 100
while x <= y:
    print(y)
    y = y - 10
print ("Программа завершилась!")

# Задание 2a (в одну строчку)

x = 0
y = 100
while x <= y:
    print(y, end=' ')
    y = y - 10
print ("Программа завершилась!")

#Задание 3

import random
excuses = ["я устал от всего","я улетел и не хотел возвращаться","на улице хмуро","хочется лежать и ничего не делать",  
           "я в отпуске","много работы","я лентяй, каких свет не видывал","мне надоело",
           "прокастинировал","могу так","плохо себя чувствал","не хотел","я плохой мальчик, но уже исправляюсь"]
for sergey in excuses: 
    phrase = "Я не успел сдать ДЗ к дедлайну, потому что"+""
    reason = random.choice(excuses) 
    result = phrase +" "+reason
    print(result)

